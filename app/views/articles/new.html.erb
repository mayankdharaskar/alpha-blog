<h1> Create a new article </h1>

<%= form_with model: @article, url: articles_path do |form| %>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>


  <p>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </p>

  <p>
  <%= form.submit :Submit %>
  </p>


<%end%>
